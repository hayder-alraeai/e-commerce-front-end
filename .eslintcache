';\nimport 'antd/dist/antd.css';\nimport CreateButton from './CreateButton'\nconst CreateCategoryModalen = () => {\n    const [isModalVisible, setIsModalVisible] = useState(false);\n    const [categoryName, setCategoryName] = useState({value: ''})\n      const handleOk = () => {\n        addCategory(categoryName.value)\n        setCategoryName({value: ''})\n        setIsModalVisible(false);\n      };\n      const showModal = () => {\n        setIsModalVisible(true);\n      };\n      const handleCancel = () => {\n        setIsModalVisible(false);\n      };\n      const addCategory = async(name) => {\n        await fetch(backendPath + '/api/categories', {\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json' \n            },\n            body: JSON.stringify({ categoryName: name }),    \n        })\n        .catch(error => console.log(error))\n    }\n    return(\n        <div>\n            <CreateButton onClick={showModal} />\n            <Modal \n                className=\"create-category-modal\"\n                title=\"Create a new Category\"\n                visible={isModalVisible} \n                onOk={handleOk} \n                bodyStyle={{backgroundColor:'#ccc'}}\n                onCancel={handleCancel}>\n                    <input \n                        type=\"text\" \n                        placeholder=\"Category name ...\"\n                        onChange={e => setCategoryName({value:e.target.value})}\n                        value={categoryName.value}\n                    />\n        </Modal>\n        </div>\n\n    )\n}\nexport default CreateCategoryModalen",{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":3,"column":24,"nodeType":"98","messageId":"99","endLine":3,"endColumn":28},{"ruleId":"100","severity":1,"message":"101","line":20,"column":46,"nodeType":"102","endLine":20,"endColumn":65},{"ruleId":"100","severity":1,"message":